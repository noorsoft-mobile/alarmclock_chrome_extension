{
  "name": "Alarm clock",
  "version": "0.0.1",
  "description": "Here will be a description",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icons": {
      "128": "images/u10.png"
    }
  },
  "icons": {
    "128": "images/u10.png"
  },
  "web_accessible_resources": [
    "music/Midnight-chimes-sound-effect.mp3",
    "music/Twin-bell-alarm-clock-sound.mp3"
	],
  "permissions": ["tabs", "declarativeContent", "alarms", "storage", "contextMenus"],
  "manifest_version": 2
}